	<queryString language="SQL">
		<![CDATA[SELECT   nh.ten ten_nh,
         ecus.ten_dv ten_kh,
         ecus.ma_dv mst,
         REPLACE(ecus.email, ']', ', ') email, 
         REPLACE(ecus.so_dt, ']', ', ') phone,
         ecus.tai_khoan_th so_tk,
         ecus.cts_serial,
         to_char(ecus.ngay_dk,'dd/mm/yyyy') ngay_dk,
         to_char(ecus.cif)  cif,
         dia_chi,
         so_dkkd,
         so_cmt,
         HO_TEN,
          (select TEN from tcs_dm_nganhang where ma_nh = BR_CODE and rownum = 1) ten,
          LYDO_HUYDV,
          to_char(NGAY_NGUNG_DV,'dd/MM/yyyy') NGAY_NGUNG_DV
  FROM   tcs_ecus_customer ecus, TCS_DM_NGANHANG nh
 WHERE   1 = 1
         AND ecus.br_code =nh.ma_nh
         AND ecus.ma_dv = $P{P_MST}  AND ecus.id= $P{P_CUS_ID}]]>
	</queryString>
