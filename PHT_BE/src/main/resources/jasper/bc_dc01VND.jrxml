	<queryString language="SQL">
		<![CDATA[SELECT sum(count_in_cot),sum(sum_in_cot),sum(count_out_cot), sum(sum_out_cot) FROM
(SELECT count(1) count_in_cot,nvl(sum(ttien),0) sum_in_cot, 0 count_out_cot, 0 sum_out_cot from
 (select id_ctu,ttien FROM tcs_dchieu_bcdc a
where ctu_tct = ctu_nh
and cot = 1 and ma_nt='VND'
AND a.ID_DC =  $P{id} 
group by id_ctu,ttien)

UNION ALL

SELECT 0 count_in_cot, 0 sum_in_cot, count(1) count_out_cot,nvl(sum(ttien),0) sum_out_cot from
 (select id_ctu,ttien FROM tcs_dchieu_bcdc a
where ctu_tct = ctu_nh
and cot = 0 and ma_nt='VND'
AND a.ID_DC= $P{id} 
group by id_ctu,ttien))]]>
	</queryString>
